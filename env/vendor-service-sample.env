VENDOR_DB_URL=jdbc:postgresql://vendor-db:5432/vendor_db
VENDOR_DB_USER=vendor_user
VENDOR_DB_PASS=vendor_pass
INTERNAL_AUTH_SHARED_SECRET=change-this-secret
SAMPLE_VENDOR_SEED_ENABLED=true
HTTP_CLIENT_CONNECT_TIMEOUT_SECONDS=2
HTTP_CLIENT_RESPONSE_TIMEOUT_SECONDS=5
HTTP_CLIENT_IDLE_EVICT_SECONDS=30
CACHE_VENDOR_OPERATIONAL_STATE_TTL=15s
VENDOR_DELETE_REFUND_HOLD_DAYS=14
VENDOR_IDEMPOTENCY_ENABLED=true
VENDOR_IDEMPOTENCY_KEY_HEADER=Idempotency-Key
VENDOR_IDEMPOTENCY_KEY_PREFIX=vs:idem:v1::
VENDOR_IDEMPOTENCY_PENDING_TTL=30s
VENDOR_IDEMPOTENCY_RESPONSE_TTL=6h
